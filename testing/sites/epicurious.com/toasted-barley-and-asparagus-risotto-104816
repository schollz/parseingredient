            <script type="text/javascript">
            (function() {
                var MAX_TIMES = 10,
                    current = 0
                    fired = false;
                function statsCall() {
                    try {
                        _satellite.pageBottom();
                        return true;
                    } catch (e) {
                        return false;
                    }
                }
            
                if (!statsCall() ) {
                    var timer = setInterval(function() {
                        current += 1;
                        if (current > MAX_TIMES) {
                            clearInterval(timer);
                        } else {
                            var result = statsCall();
                            if (result) {
                                clearInterval(timer);
                            }
                        }
                    }, 200);
                }
            
            })();
            </script>
