        <div class="field-group__hover pull-right subtle editor__tool editor__tool--pc">
          <a class="faint" tabindex="-1" data-behavior="delete" data-confirm="Are you sure you want to delete this?" href="#"><i class="icf icf_close "></i></a>
          <i data-behavior="handle" class="icf icf_sortable "></i>
        </div>
      </div>

      <div class="step__image-mobile visible-xs-block hidden-print">
        
      </div>
    </div>

  </li>
  <li class="step numbered-list__item card card--sm" data-noteable="#note_list_step_3320075" id="step_3320075" data-record="/us/recipes/750041-backyard-bbq-chex-mix/steps">
