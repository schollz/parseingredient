          document.getElementById("playerLightbox").className = "playerShow";
          if (iOS) {
            document.getElementById("myExperience4724779710001").className = "BrightcoveExperience";
          }

          player = brightcove.api.getExperience("myExperience4724779710001");
          modVP = player.getModule(brightcove.api.modules.APIModules.VIDEO_PLAYER);
          modVP.loadVideoByID(videoId);
        } 
        var hideAndStop = function() {
          modVP.pause();
          nextVideo = 0;
          document.getElementById("playerLightbox").className = "playerHide";
          if (iOS) {
            document.getElementById("myExperience4724779710001").className = "playerHide";
          }
