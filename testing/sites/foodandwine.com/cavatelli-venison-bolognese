              $("#phone-lazy-load-first-ad-holder").hide();
              $("#phone-lazy-load-ad-holder").hide();
              $("#tablet-lazy-load-first-ad-holder").hide();
              $("#tablet-lazy-load-ad-holder").hide();
            }  
