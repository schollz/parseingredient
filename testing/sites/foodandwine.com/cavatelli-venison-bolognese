        <script type="text/javascript">
          if (detect_device == "desktop") {
            if (typeof(window.pageAdSlots) == "undefined") {
              window.pageAdSlots = ['adTop'];
            } else {
              window.pageAdSlots.push('adTop');
            }                   
            if (jQuery(".node-type-gallery").length > 0) {
              ad = adFactory.getMultiAd(new Array("101x1", "970x90", "970x66", "728x90"));
            }else{
              ad = adFactory.getMultiAd(new Array("101x1", "970x250", "970x90", "970x66", "728x90"));

            }
            ad.setPosition("1");
            ad.write("adTop");
          }
          else if (detect_device == "tablet") {
            if (typeof(window.pageAdSlots) == "undefined") {
              window.pageAdSlots = ['adTop'];
            } else {
              window.pageAdSlots.push('adTop');
            }                   
            ad = adFactory.getMultiAd(new Array("728x90"));
            ad.setPosition("1");
            ad.write("adTop");
          }
          </script>
