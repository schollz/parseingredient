    url: $(this).attr("href"),
    dataType: "script",
    error: function(jqXHR, textStatus, errorThrown){
    	alert(textStatus+': '+errorThrown);
    },
    complete: function(){$("#rcs_47").hide();}
  });
  e.preventDefault();
});
