<script>
$("#c1_link").click(function(e){
  e.preventDefault();
  $("#comment_form1").hide();
  $.ajax({
    url: $(this).attr("href"),
    dataType: 'script',
    error: function(jqXHR, textStatus, errorThrown){
      alert(textStatus+': '+errorThrown);
    },
    complete: function(){
      $("#comment_form1").slideDown('fast',function(){
        $('#cf-body').focus();
      });
    }
  });
});
</script>
<script>
$(document).ready(function(){
	$("#tick").load("/live",function(){
		var w = document.body.offsetWidth;
		if ( w < 992) {
	 		$('#tick-box-float').css({bottom: '0px'});  
	 	}
	});	
});
</script>



