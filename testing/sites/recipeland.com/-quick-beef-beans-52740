	function print() {
  var form = document.createElement("form");
	var href = '/recipes/recipe/print_full/' + encodeURIComponent("52740");
	form.setAttribute("method", "post");
	form.setAttribute("action", href);
	form.setAttribute("target", "formresult");
	var hiddenField = document.createElement("input");              
	hiddenField.setAttribute("type", "hidden");
	hiddenField.setAttribute("name", "authenticity_token");
	hiddenField.setAttribute("value", "jTwcfQwMBApcbAWhrMS6C2I1XERGAlYUm8upC44IbnU=");
	form.appendChild(hiddenField);
	document.body.appendChild(form);
	window.open('/images/blank.gif', 'formresult', 'scrollbars=yes,menubar=yes,height=600,width=800,resizable=yes,toolbar=no,status=no');
	form.submit();		
	}


	$("#rab-print").click(function(e){
		$(this).toggleClass('rotate');
		e.preventDefault();
		print();
	});

	$("#rab-photo").click(function(){
		$(this).toggleClass('rotate');
		var cb = $("#photo-dialog");
		var rab = $("#rab");
		var offset = rab.offset();
		var w = rab.width();
		cb.css('top',offset.top+'px');
		cb.css('left',offset.left+'px');
		cb.css('height','auto');
		cb.css('width', 'auto');
		cb.fadeIn('fast');
		$("#lights-out").fadeIn('fast');

		//window.location = "/recipes/recipe/52740/add_photo";
	});

	$("#shr-fb").click(function(){
