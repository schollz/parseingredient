<script type="text/javascript">
function clear_search_icons() {
	$("#ts_rm").attr("class","icon_6");
	$("#ts_tm").attr("class","icon_6");
	$("#ts_im").attr("class","icon_6");
	$("#ts_cm").attr("class","icon_6");
	$("#ts_am").attr("class","icon_6");
}


$("#ts_sr").click(function(){
	$("#ts_form").attr("action","https://recipeland.com/recipes/recipe/search");
	$("#ts_type").attr("value","Recipes")
	clear_search_icons();
	$("#ts_rm").attr("class","icon-caret-right");
	$('#ts_q').attr("placeholder",'Find a recipe…');
	$("#ts_q").focus();
	return false;
});
$("#ts_si").click(function(){
	$("#ts_form").attr("action","https://recipeland.com/recipes/ingredients/search");
	$("#ts_type").attr("value","Ingredients")
	clear_search_icons();
	$("#ts_im").attr("class","icon-caret-right");
	$('#ts_q').attr("placeholder",'by Ingredients…');
	$("#ts_q").focus();
	return false;
});
$("#ts_st").click(function(){
	$("#ts_form").attr("action","https://recipeland.com/recipes/recipe/search");
	$("#ts_type").attr("value","Recipe Title")
	clear_search_icons();
	$("#ts_tm").attr("class","icon-caret-right");
	$('#ts_q').attr("placeholder",'by Title…')
	$("#ts_q").focus();
	return false;
});
$("#ts_sc").click(function(){
	$("#ts_form").attr("action","https://recipeland.com/recipes/categories/search");
	$("#ts_type").attr("value","Categories")
	clear_search_icons();
	$("#ts_cm").attr("class","icon-caret-right");	
	$("#ts_q").focus();
	return false;
});
$("#ts_sa").click(function(){
	$("#ts_form").attr("action","https://recipeland.com/howto/search");
	$("#ts_type").attr("value","Articles")
	clear_search_icons();
	$("#ts_am").attr("class","icon-caret-right");	
	$('#ts_q').attr("placeholder",'Search cooking tips…');
	$("#ts_q").focus();
	return false;
});
$("#ts_form").submit(function(){
	$("#ts_ss").show();
});
</script>

