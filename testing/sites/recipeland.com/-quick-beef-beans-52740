		tweet_url += "&url=https%3A%2F%2Frecipeland.com%2Frecipe%2Fv%2F-quick-beef-beans-52740";
		window.open(tweet_url,'_blank','menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
	});

	$("#shr-glgl").click(function(){
		$(this).toggleClass('rotate');
		plus_url = "https://plus.google.com/share?url="
		plus_url += "https%3A%2F%2Frecipeland.com%2Frecipe%2Fv%2F-quick-beef-beans-52740"
