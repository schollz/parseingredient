<script type="text/javascript">
    function SendSecureLogin(validationGroup) {
        var validationResult;
        document.forms[0].action = modalLoginUrl;
        jQuery(".left_container input").removeClass("invalid_text");
        if (typeof (Page_ClientValidate) == 'function') {
           //we override the scrollTo because asp.net validtion script auto scrolls to top, which we do not want 
           var oldScrollTo = window.scrollTo;
           window.scrollTo = function() {};

           validationResult = Page_ClientValidate(validationGroup);            

           window.scrollTo = oldScrollTo;
        }
        if (validationResult) {
           document.forms[0].submit();
        } else {            
           showValidationErrors(validationGroup);
        }  
    }

    function saveLoginMessage(message) {
        if (typeof (message) != "undefined" && message !== null) {
            var arr = message.split('|');
            if(arr.length != 0){
                if(arr.length > 1)
                    jQuery("#" + headLineId).text(arr[1]); 
                jQuery("#" + instructionsId).html(arr[0]);                    
            }
            else{
                jQuery("#" + instructionsId).html(message);
            }
        }
    }
     
    function saveRegistrationParameters(registrationSource, destinationUrl, regAction) {
        var params = {
            esrc: registrationSource || 15846,
            returnUrl: destinationUrl || document.location.href
            //isRegistrationFirstStep: false
        };
        if (regAction) {
            params.RegAction = regAction;
        }
        var paramsString = JSON.stringify(params);
        jQuery('#' + registrationParamsHiddenFieldId).val(paramsString);
    }
    
    function openLogin(registrationSource, message, destinationUrl, modalAction, regAction, email) {   
        if (email == undefined) {
            email = '';
        }
        var params = {
            esrc: registrationSource || 15846,
            returnUrl: destinationUrl || document.location.href,
            isRegistrationFirstStep: true,
            Email: email
        };
        if (regAction) {
            params.RegAction = regAction;
        }
        
        reg.userRegistration.openRegistrationDialog(params);
    }

    function openLoginWLogInFlow(registrationSource, message, destinationUrl, modalAction, regAction) {       
        var params = {
            esrc: registrationSource || 15846,
            returnUrl: destinationUrl || document.location.href,
            isRegistrationFirstStep: false
        };
        if (regAction) {
            params.RegAction = regAction;
        }
        reg.userRegistration.openRegistrationDialog(params);
    }

    function closeLogin() {
        reg.userRegistration.closeRegistartionDialog(params);
            
    }
          
