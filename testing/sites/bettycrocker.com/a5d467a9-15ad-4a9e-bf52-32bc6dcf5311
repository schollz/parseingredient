<script type="text/javascript">		
	(function($){
		$(function(){
			var scrollingDelegates = '.contentSubmit-TocBackToTopContainer, .TableOfContents, .youTube-WatchVideo-ButtonContainer',
                scrollEndEvents = 'scrollEnd.contentSubmitToC scrollEnd.youTube',
				scrollBeginEvents = 'scrollBegin.contentSubmitToC scrollBegin.youTube',
	                      
                scrollingElements = 'a, input, button',
				isHeaderCtaVisible;
		
			$(scrollingDelegates).on(scrollBeginEvents, scrollingElements, function () {                                  				
				$(".headroom").removeClass("headroom--pinned").addClass("headroom--unpinned").addClass("youtubeScrolling-disableTransitions");             
			});
	   
			$(scrollingDelegates).on(scrollEndEvents, scrollingElements, function () {   
				isHeaderCtaVisible = $('.newsletterCtaWrapper') && $('.newsletterCtaWrapper').height() > 0;
				if (isHeaderCtaVisible){
					$(window).scrollTop($(window).scrollTop()- 25);
				}				 
				setTimeout(function () {
					$(".headroom").removeClass("headroom--pinned").addClass("headroom--unpinned");
				}, 500);
				setTimeout(function() {
					$(".headroom").removeClass("youtubeScrolling-disableTransitions");
				}, 1000);
			});
	});		
})(jQuery);   
</script>
