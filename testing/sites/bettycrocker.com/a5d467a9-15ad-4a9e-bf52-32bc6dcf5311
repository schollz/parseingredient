<!-- Temporary fix. Added after F15 Sprint 9 release.  
Override implementation of sharebar toggle for tablet view.  -->
<script type="text/javascript">
(function($) {	
        $('body').on('click', '.contentDetail .share', function() {

	var $shareBar = $('.contentDetail .share');
 	var isClickInsideShareContainer = $(event.target).closest('.verticalButtonContainer').length;
	var isClickOnShareHeader = $(event.target).closest('.verticalButtonContainerHeader').length;

        if (isClickOnShareHeader && $shareBar.is('.closed')) {
	    $shareBar.removeClass('closed');
	}
	else if (!isClickInsideShareContainer) {
	    $shareBar.addClass('closed');
	}
        return false;	
    });
})(jQuery);
</script>
<!-- End temporary fix.  -->
