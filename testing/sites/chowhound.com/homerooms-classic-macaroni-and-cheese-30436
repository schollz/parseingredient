    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];

    xfp_id = '8264/aw-chowhound/recipes';


            googletag.cmd.push(function() {

                var env = is_prod ? "production" : "staging"


                  leader_plus_top=googletag.defineSlot("8264/aw-chowhound/recipes", [[728,90],[970,66],[728,91],[970,250]], "leader_plus_top").addService(googletag.pubads()).setTargeting("pos", "top")
                          .setCollapseEmptyDiv(true,true);


                  mpu_plus_top=googletag.defineSlot("8264/aw-chowhound/recipes", [[300, 250],[300, 600]], "mpu_plus_top").addService(googletag.pubads()).setTargeting("pos", "top");

                    sharethrough_key = "54ee49f3";
                    sharethrough=googletag.defineSlot("8264/aw-chowhound/recipes", [11, 11], "strnative_top").addService(googletag.pubads()).setTargeting("pos", "top").setTargeting("strnativekey", "54ee49f3");

                  mpu_bottom=googletag.defineSlot("8264/aw-chowhound/recipes", [300, 250], "mpu_bottom").addService(googletag.pubads()).setTargeting("pos", "bottom");

                  leader_bottom=googletag.defineSlot("8264/aw-chowhound/recipes", [[728,90],[970,66],[728,92]], "leader_bottom").addService(googletag.pubads()).setTargeting("pos", "bottom");



                googletag.pubads().setTargeting("ptype", "section_detail");

                    googletag.pubads().setTargeting("tag", "pasta,mac-and-cheese,dinner,cheese,comfort-food,easy,super-bowl,christmas,potluck,macaroni,milk,butter,side-dish,flour,winter,from-scratch,panko-breadcrumbs");
                    utag_taglist = "pasta|mac-and-cheese|dinner|cheese|comfort-food|easy|super-bowl|christmas|potluck|macaroni|milk|butter|side-dish|flour|winter|from-scratch|panko-breadcrumbs";
                    utag_data.tags = utag_taglist;


                googletag.pubads().setTargeting("loggedin", "0");googletag.pubads().setTargeting("cid", "homerooms-classic-macaroni-and-cheese");
                googletag.pubads().setTargeting("env", env);
                googletag.pubads().setTargeting("vguid", view_tracking_guid); 
                if (tags.ftag) {
                    googletag.pubads().setTargeting("ftag", tags.ftag);
                }

                googletag.pubads().enableAsyncRendering();
                googletag.pubads().enableSingleRequest();
                googletag.pubads().collapseEmptyDivs();
                googletag.pubads().disableInitialLoad();
                googletag.enableServices();
            });


            googletag.cmd.push(function() {
            if(typeof index_headertag_lightspeed !== 'undefined') {
                index_headertag_lightspeed.add_session_end_hook(function(){
                var slots = googletag.pubads().getSlots();
                index_headertag_lightspeed.set_slot_targeting(slots);

                
                  googletag.display("leader_plus_top");


                  googletag.display("mpu_plus_top");

                  googletag.display("mpu_bottom");

                  googletag.display("leader_bottom");



                  googletag.pubads().refresh();

                }, true);
                    index_headertag_lightspeed.refresh();
                } else {

                
                  googletag.display("leader_plus_top");


                  googletag.display("mpu_plus_top");

                  googletag.display("mpu_bottom");

                  googletag.display("leader_bottom");



                    googletag.pubads().refresh();

                }
            });
            

